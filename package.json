{
  "name": "@momwins/mom-design-system",
  "version": "2.3.19",
  "description": "Design system for Ministry of Manpower Work Pass Integrated Systems (WINS)",
  "author": "MOM",
  "main": "dist/system/system.js",
  "files": [
    "dist/system"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab-dev.wins.momdev.gcc.gov.sg:8081/gitlab/design_system/mom_design_system.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "init": "npm run gulp && npm run build",
    "start": "npm run gulp:watch & npm run styleguide & npm run dev",
    "dev": "webpack serve --inline --config ./build/dev.js",
    "build": "node ./build/build.js",
    "build:styleguide": "vue-styleguidist build --config styleguide/config.js",
    "build:all": "npm run build && npm run build:styleguide",
    "styleguide": "vue-styleguidist server --open --config styleguide/config.js",
    "gulp": "gulp --gulpfile ./gulpfile.js",
    "gulp:tokens": "gulp tokens --gulpfile ./gulpfile.js",
    "gulp:assets": "gulp assets --gulpfile ./gulpfile.js",
    "gulp:global": "gulp global --gulpfile ./gulpfile.js",
    "gulp:carapace": "gulp carapace --gulpfile ./gulpfile.js",
    "gulp:watch": "gulp watch --gulpfile ./gulpfile.js",
    "backstop": "backstop test --config=test/backstop/config",
    "backstop:approve": "backstop approve --config=test/backstop/config",
    "jest": "jest --config=test/jest/config.js",
    "jest:watch": "jest --watch --config=test/jest/config.js",
    "test": "npm run jest && npm run backstop"
  },
  "keywords": [
    "mom design system"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
